/***头部菜单整体***/
.navigation{
	height: 45px;
	background: #395a9d !important;
    border-bottom: 1px solid #244180 !important;
	box-sizing: border-box;
}
.navigation ul li{
	float: left;
	line-height: 43px;
	color: #e4edfc;  /****|的颜色****/
}
/****字体的颜色****/
.navigation ul li a{color: #e4edfc;}

.navigation ul li a:hover{text-decoration: underline;}
/***头部左侧部分***/
.navigation .top-left{margin-left: 20px;}
.navigation .top-left li a{
	padding: 0 15px;
	font-size: 17px;
	/*font-weight: 600;*/
}

/*****下拉菜单******/
/*****easu ui下拉菜单样式重置*******/
.menu-line{display:none;}
a.easyui-menubutton.l-btn.l-btn-plain{
    height: 43px;
    padding-top: 8px;
    box-sizing: border-box;
    border-radius: 0;
    border:none;
}
.top-right a.easyui-menubutton.m-btn-plain-active,
.top-right a.easyui-menubutton.l-btn-plain:hover,
.top-right a.easyui-menubutton.l-btn:hover,
.top-right a.easyui-menubutton.l-btn-plain:focus,
.top-right a.easyui-menubutton.l-btn:focus{
	background:#244180;
}
/*****下拉按钮*******/
.dropdown a .dropdown-text{
	color: #e4edfc;
	/* font-size: 17px; */
}
/*****下拉列表*******/
.dropdown-menu{
	border:1px solid #d7ebfe;
	border-top:none;
	background:#fff;
}
.dropdown-menu li a{
	line-height: 30px !important;
    padding: 0 10px;
    color:#666;
}
.dropdown-menu li{display:block;}
/**下拉列表选种样式**/
.dropdown-menu li:hover,.dropdown-menu li:focus{background:#eaf2ff;}
.dropdown-menu li:hover a,.dropdown-menu li:focus a{color: #0e59bf;}
/**三角**/
/**三角隐藏easy ui的样式**/
.m-btn-downarrow, .s-btn-downarrow{background:none;margin-top: -12px;}
/**三角重置样式**/
.m-btn-downarrow:before, .s-btn-downarrow:before{
     content: "\f0dd";
     font: normal normal normal 14px/1 FontAwesome;
     color: #e4edfc;
}
/*****皮肤下拉菜单*****/
.dropdown-menu-skin{min-width: 80px !important;}
.dropdown-menu-skin li {text-align: center;}

/***头部右侧部分***/
.navigation .top-right{float: right;}
.navigation .top-right li a{
	padding: 0 10px;
	font-size: 14px;
}

/***消息中心***/
.circle{
	width: 16px;
	height: 16px;
	border-radius: 50%;
	display: inline-block;
	color: #fff;
	right: 15px;
    top: 13px;
    line-height: 16px;
    text-align: center;
    background: #ffac26;
}

/*********全文检索 start **********/
.index-searchBox{ 
	position: relative;
	margin-right:20px;
}
.index-searchBox input::-webkit-input-placeholder { /* WebKit browsers */ 
color: #a3b8d6; 
} 
.index-searchBox input:-moz-placeholder { /* Mozilla Firefox 4 to 18 */ 
color: #a3b8d6; 
} 
.index-searchBox input::-moz-placeholder { /* Mozilla Firefox 19+ */ 
color: #a3b8d6; 
} 
.index-searchBox input:-ms-input-placeholder { /* Internet Explorer 10+ */ 
color: #a3b8d6; 
} 
.index-searchBox input{
    width:190px;
    height: 26px;
    padding-left: 10px;
    border:1px solid #57749c;
    border-radius: 13px;
    outline: none;
    background: #244180;
    color: #e4edfc;
}
.index-searchBox button[type="submit"]{
    position: absolute;
    top: 8px;
    right: 2px;
    width: 26px;
    height: 26px;
    border: none;
    cursor: pointer;
    background: none;
}
.index-searchBox button[type="submit"]:before{
    content: "\f002";
    font-family: FontAwesome;
    color: #a3b8d6;
}
/*********全文检索 end **********/


/**面包屑导航**/
.breadcrumb{
	height: 38px;
	line-height: 36px;
	background: #fefefe;
	border-bottom:1px solid #d9d9d9;
    box-sizing: border-box;
}
.breadcrumb .fa{margin:0 5px 0 10px;}
/**右侧登录时间**/
.breadcrumb .login-time{
	float: right;
	color: #999;
	margin-right: 10px;
}

