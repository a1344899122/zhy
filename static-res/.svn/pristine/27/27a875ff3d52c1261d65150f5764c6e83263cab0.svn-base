
/*******头部tab ul********/
#myTabs{
	border-bottom: 1px solid #2068c7;
	height: 36px !important;
	padding: 0;
	background: #f4f7fc;
    width: 100% !important;
    padding-left: 10px;
    box-sizing: border-box;
}
#myTabs li{
    margin: 6px 4px -1px 0;
    float: left;
}
/*****tab按钮样式*****/
#myTabs li a{
	font-size: 14px;
	color: #444;
	height: 29px !important;
	line-height: 29px !important;
	background: #fefefe;
	border:1px solid #ccc;
	border-bottom: none;
	padding:5px 12px;
}
/*****tab按钮样式――选中*****/
#myTabs li.active{
	margin-top:7px\0;
}
#myTabs li.active a{
	border-top-width:2px;
	border-color: #317bde;
	background: #fefefe;
	border-bottom: 1px solid #fefefe;
	color: #2068c7;
	z-index:2;
}

/*******切换内容部分********/
#myTabContent{
	border: none;
	/* height: calc(100vh - 40px); */
	width: 100% !important;
}
/*******内容部分小内容********/
#myTabContent div{
	height:100%;
	width: 100% !important;
	display:none;
}
/***内容*****/
#tabs-panels .panel-body{
	background: none;
	height:100%;
	
}
#myTabContent .mCSB_inside > .mCSB_container{	
	padding-bottom:26px\0; /****针对IE10以下****/
}
#model_tab_container{
	padding:10px;
}


