*[disabled]{
	color: #aaa!important;
	cursor: not-allowed;
}

/***按钮整体样式***/
button{
	cursor: pointer;
	border-radius: 0;
	color: #fff;
}
button + button{
	margin-left: 5px;
}

.btn-primary i,.btn-warning i,.btn-info i,.btn-success i,.btn-danger i{color: #fff;}
/**正常尺寸**/
.btn{padding:2px 13px 4px 13px;}
	
/**大尺寸**/
.btn-lg{padding:4px 17px 6px 17px;font-size: 14px;height: 32px;}
/**小尺寸**/
.btn-small{
	font-family: "宋体";
	font-size: 12px;
	padding:3px 7px;
	color: #2068c7 !important;
	border-top: 1px #6da7f2 solid;
    border-left: 1px #6da7f2 solid;
    border-right: 1px #6da7f2 solid;
    border-bottom: 1px #6da7f2 solid;
    background: url(../images/button-small.jpg) left center repeat-x;
}
.btn-small[disabled]{
	color: #aaa!important;
	cursor: not-allowed;
	
	border-top: 1px #c0c0c0 solid;
    border-left: 1px #c0c0c0 solid;
    border-right: 1px #c0c0c0 solid;
    border-bottom: 1px #c0c0c0 solid;
}

.btn-small i{color: #2068c7 !important;}


.btn-bigsmall{
	font-family: "宋体";
	font-size: 12px;
	padding:4px 8px;
	color: #2068c7 !important;
