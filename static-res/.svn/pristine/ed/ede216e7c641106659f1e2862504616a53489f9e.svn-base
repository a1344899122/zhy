	margin-left:0;
	margin-top:6px;
}
.more-and-remove .btn{
	padding: 2px 9px 4px 9px;
}


/*******开关按钮********/
#funSetForm .panel{
	position: relative;
}
.toggle-button-wrapper{
	position: absolute;
	right:8px;
	top:8px;
}
.toggle-button-wrapper .toggle-button{ display: none; }
.toggle-button-wrapper .button-label{
    position: relative;
    display: inline-block;
    width:90px;
    box-sizing:border-box;
    padding:0 10px;
    height: 20px;
    background-color: #aaa;
    box-shadow: #ccc 0px 0px 0px 1px;
    border-radius: 10px;
    overflow: hidden;
}
.toggle-button-wrapper .circle{
    position: absolute;
    top: 0;
    left: 0;
    width: 20px;
    height: 20px;
    border-radius: 50%;
    background-color: #fff;
}
.toggle-button-wrapper .button-label .text {
    line-height: 18px;
    font-size: 13px;
    text-shadow: 0 0 1px #ddd;
}

.toggle-button-wrapper .on { color: #fff; display: none;}
.toggle-button-wrapper .off { color: #fff; display: inline-block; text-indent:16px;}
.toggle-button-wrapper .button-label .circle{
    left: 0;
    transition: all 0.3s;
}
.toggle-button-wrapper .toggle-button:checked + label.button-label .circle{
    left: 70px;
    background: #f4f7fc;
}
.toggle-button-wrapper .toggle-button:checked + label.button-label .on{ display: inline-block; }
.toggle-button-wrapper .toggle-button:checked + label.button-label .off{ display: none; }
.toggle-button-wrapper .toggle-button:checked + label.button-label{
    background-color: #317bde;
}


/********进度条*********/
.progress {
    height: 20px;
    overflow: hidden;
    background-color: #f5f5f5;
    -webkit-box-shadow: inset 0 1px 2px rgba(0,0,0,.1);
    box-shadow: inset 0 1px 2px rgba(0,0,0,.1);
}
.progress-bar {
    float: left;
    width: 0;
    height: 100%;
    font-size: 12px;
    line-height: 20px;
    color: #fff;
    text-align: center;
    background-color: #50ce8a;
    -webkit-box-shadow: inset 0 -1px 0 rgba(0,0,0,.15);
    box-shadow: inset 0 -1px 0 rgba(0,0,0,.15);
    -webkit-transition: width .6s ease;
    -o-transition: width .6s ease;
    transition: width .6s ease;
}

/******下载附件*******/
.download-file{
    padding: 2px;
    background: #fcfcfc;
    margin-top: 4px;
    border: 1px solid #f5f5f5;
    box-sizing: border-box;
   	position: relative;
}
/*预览*/
.download-file .preview-file{color:#333;font-size: 13px;}
.download-file .preview-file i{
	color: #0e59bf;
    margin-right: 3px;
}
.download-file .preview-file:hover,
.download-file .preview-file:focus{color: #0e59bf;}
/*删除*/
.download-file .del-file{
    position: absolute;
    right: 0;
    top: 0;
    height: 23px;
    width: 23px;
    text-align: center;
    padding-top:4px;
    box-sizing: border-box;
    background-color: #f2dede;
    border-color: #ebccd1;
}
.download-file .del-file i{
    color: #a94442;
}
.download-file .del-file:hover{
    background-color: #a94442;
}
.download-file .del-file:hover i{
    color: #fff;
}
/*下载*/
.download-file .btn-download{
	position: absolute;
    right: 23px;
    top: 0;
    height: 23px;
    width: 23px;
    text-align: center;
    padding-top:4px;
    box-sizing: border-box;
    background-color: #dff0d8;
    border-color: #d6e9c6;
}
.download-file .btn-download i{
    color: #3c763d;
}
.download-file .btn-download:hover{
    background-color: #3c763d;
}
.download-file .btn-download:hover i{
    color: #fff;
}


